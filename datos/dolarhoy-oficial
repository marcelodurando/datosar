                apiKey: "AIzaSyCIr25LfMkJNICfQ27aGbDl7DfEkwNYYhU",
                authDomain: "dolarhoy-fb48e.firebaseapp.com",
                projectId: "dolarhoy-fb48e",
                storageBucket: "dolarhoy-fb48e.appspot.com",
                messagingSenderId: "671441363501",
                appId: "1:671441363501:web:9332041d1489ada4c04ea4",
